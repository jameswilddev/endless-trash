{
  "name": "@endless-trash/validator",
  "description": "Validates the request body against a JSON schema.",
  "version": "0.0.0",
  "engines": {
    "node": ">=10.19.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/endless-trash"
  },
  "license": "MIT",
  "dependencies": {
    "ajv": "7.2.1"
  },
  "devDependencies": {
    "@endless-trash/event-handler": "0.0.2",
    "@endless-trash/immutable-json-type": "0.0.2",
    "@types/json-schema": "7.0.7"
  },
  "types": "index.d.ts",
  "sideEffects": false
}
